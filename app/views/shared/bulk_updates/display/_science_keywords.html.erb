<% if bulk_update['update-type'] == 'FIND_AND_REMOVE' || bulk_update['update-type'] == 'FIND_AND_REPLACE' %>
  <!-- Find Value -->
  <%= display_value_title(bulk_update['update-type'], 'find') %>

  <ul class="arrow-tag-group-list">
    <% display_science_keyword(bulk_update['find-value']).each do |field, value| %>
      <li itemprop="keyword" class="arrow-tag-group-item"><%= value %></li>
    <% end %>
  </ul>
<% end %>

<% unless bulk_update['update_type'] == 'FIND_AND_REMOVE' %>
  <!-- Update Value -->
  <%= display_value_title(bulk_update['update-type'], 'new') %>

  <ul class="arrow-tag-group-list">
    <% display_science_keyword(bulk_update['update-value']).each do |field, value| %>
      <li itemprop="keyword" class="arrow-tag-group-item"><%= value %></li>
    <% end %>
  </ul>
<% end %>